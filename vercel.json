{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.starrupturecalculator.com" }],
      "destination": "https://starrupturecalculator.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "satisfactorytools.com" }],
      "destination": "https://starrupturecalculator.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.satisfactorytools.com" }],
      "destination": "https://starrupturecalculator.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/www/assets/:path*" },
    { "source": "/img/:path*", "destination": "/www/img/:path*" },
    { "source": "/robots.txt", "destination": "/www/robots.txt" },
    { "source": "/sitemap.xml", "destination": "/www/sitemap.xml" },
    { "source": "/sitemap-:name*.xml", "destination": "/www/sitemap-:name*.xml" },
    { "source": "/favicon.ico", "destination": "/www/favicon.ico" },
    { "source": "/index.html", "destination": "/www/index.html" },
    { "source": "/:path*", "destination": "/www/index.html" }
  ]
}

